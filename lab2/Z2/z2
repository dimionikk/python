import math
from mod import product

def expression(m):
    return math.sqrt((m + 3) / (m - 3))

m =                     int(input("введіть число m(m<-3 або m>3): "))
while -3 <= m <= 3: m = int(input("введіть число m (m<-3 або m>3): "))
print("значення z =", expression(m))

n =               int(input("введіть натуральне число n: "))
while n <= 0: n = int(input("введіть натуральне число n: "))
print("y =", product(n))
